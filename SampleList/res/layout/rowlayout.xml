<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="wrap_content">
	<ImageView android:id="@+id/icon" android:layout_height="wrap_content"
		android:src="@drawable/icon" android:layout_width="40px"
		android:layout_marginTop="0px" android:layout_marginRight="12px"
		android:layout_marginLeft="12px">
	</ImageView>
	<TextView android:text="@+id/TextView01" android:layout_width="200dip"
		android:layout_height="fill_parent" android:id="@+id/label"
		android:gravity="center_vertical" android:textSize="30px"></TextView>
	<CheckBox android:id="@+id/check" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_marginLeft="4px"
		android:layout_marginRight="10px" android:layout_alignParentRight="true"></CheckBox>
</LinearLayout>